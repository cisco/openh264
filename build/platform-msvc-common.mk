AR_OPTS=-nologo -out:$@
CFLAGS_OPT=-O2 -Ob1 -Oy- -Zi -GF -Gm- -GS -Gy -MD -DNDEBUG
CFLAGS_DEBUG=-Od -Oy- -ZI -Gm -MDd -RTC1 -D_DEBUG
